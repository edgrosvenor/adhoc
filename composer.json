{
    "name": "edgrosvenor/adhoc",
    "description": "Allow Laravel commands to accept ad hoc arguments and options",
    "keywords": [
        "laravel"
    ],
    "homepage": "https://github.com/edgrosvenor/adhoc",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ed Grosvenor",
            "email": "ed.grosvenor@wickedviral.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6|^6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Adhocparam\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adhoc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adhoc\\AdhocServiceProvider"
            ]
        }
    }
}
